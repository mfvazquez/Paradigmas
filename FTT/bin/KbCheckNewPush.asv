function apretados = KbCheckNewPush(anteriores)
    [~, ~, keyCode, ~] = KbCheck;
    teclas_activas = find(keyCode);
    anteriores_sub = find(anteriores);
    
    if ~isempty(anteriores_sub)
        apretados = setdiff(teclas_activas, anteriores_sub);
    end 
    
    if isempty()
        apretados = keyCode;
        display 'caca'
        return
    end
    
    display 'pedo'
    apretados = zeros(1, 256);
    for i = 1:length(apretados_sub)
        apretados(i) = 1;
    end
    
end
